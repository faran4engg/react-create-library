{
	"name": "{{name}}",
	{{#ifCond author '!==' ''}}
	"homepage": "http://{{author}}.github.io/{{name}}",
	{{/ifCond}}
	"version": "0.0.1",
	"description": {{description}},
	{{#ifCond author '!==' ''}}
	"repository": {
		"type": "git",
		"url": {{#ifCond repo '!==' ''}}"https://github.com/{{repo}}"{{else}}"https://github.com/{{author}}/{{name}}"{{/ifCond}}
	},
	{{/ifCond}}
	"main": "dist/cjs/index",
	"module": "dist/es/index",
	{{#if externalCSS}}
	"style": "dist/{{name}}.css",
	{{/if}}
	{{#ifCond typeSystem '===' 'typescript'}}"types": "dist/types/index.d.ts",{{/ifCond}}
	"sideEffects": false,
	"author": {
		"name": "{{author}}",
		"email": "{{email}}"
	},
	"scripts": {
		{{#ifCond typeSystem '===' 'flow'}}
		"flow": "flow",
		{{/ifCond}}
		"installPeer": "node build-tool/peerDep.js",
		"test": "jest",
		"test:dev": "jest --watch",
		"lint": {{#ifCond typeSystem '===' 'typescript'}}"eslint src/**/*.{js,tsx,ts,jsx} --fix"{{else}}"eslint src/**/*.{js,jsx} --fix"{{/ifCond}},
		"stylelint": "stylelint src/**/*.{ts,tsx,jsx,js,css,scss,less} --fix",
		"commit": "git-cz",
		"release": "standard-version",
		"release:tags": "git push --follow-tags origin master",
		"build": "node build-tool/build.js",
		"pre-publish": "npm run lint && npm run build",
		"publish": "npm run pre-publish && npm publish"
		{{#ifCond documentation '===' 'none'}}
		{{else}}
		,"doc:dev": "{{#ifCond documentation '===' 'docz'}}docz dev {{else}} {{#ifCond documentation '===' 'storybook'}} start-storybook -p 6006 {{else}} styleguidist server{{/ifCond}}{{/ifCond}}",
		"doc:build": "{{#ifCond documentation '===' 'docz'}}docz build {{else}} {{#ifCond documentation '===' 'storybook'}} build-storybook --output-dir docs {{else}} styleguidist build{{/ifCond}}{{/ifCond}}",
		"doc:deploy": "npm run doc:build && gh-pages -d docs"
		{{/ifCond}}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"prop-types": "^15.7.2"
		{{#ifCond style '===' 'emotion'}}
		,"@emotion/core": "^10.0.22"
		{{/ifCond}}
		{{#ifCond style '===' 'styled-component'}}
		,"styled-components": "^4.4.1"
		{{/ifCond}}
	},
	"license": "{{license}}",
	"engines": {
		"node": ">=8.0.0"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged && git add ."
		}
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"{src,docs}/**/*.{js,json,css,jsx,ts,tsx} *.js": [
			"pretty-quick --staged",
			"npm run stylelint",
			"eslint --fix --quiet --ignore-path .gitignore",
			"git add"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.7.0",
		"@babel/core": "^7.7.2",
		"@babel/plugin-proposal-class-properties": "^7.7.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.6.2",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.7.1",
		"@babel/preset-react": "^7.7.0",
		"@commitlint/cli": "^8.2.0",
		"@commitlint/config-conventional": "^8.2.0",
		"@rollup/plugin-replace": "^2.2.1",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^24.9.0",
		"babel-plugin-minify-dead-code-elimination": "^0.5.1",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24",
		"commitizen": "^4.0.3",
		"eslint": "^6.6.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-config-prettier": "^6.7.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^1.0.1",
		"husky": "^3.0.9",
		"jest": "^24.9.0",
		"lint-staged": "^9.4.2",
		"prettier": "^1.19.1",
		"pretty-quick": "^2.0.1",
		"react-test-renderer": "^16.12.0",
		"rollup": "^1.26.5",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-peer-deps-external": "^2.2.0",
		"rollup-plugin-size-snapshot": "^0.10.0",
		"rollup-plugin-terser": "^5.1.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"prop-types": "^15.7.2",
		"standard-version": "^7.0.0",
		"stylelint": "^12.0.0",
		"stylelint-config-standard": "^19.0.0",
		"concurrently": "^5.0.0",
		"cross-spawn": "^7.0.1",
		"glob": "^7.1.6",
		"inflected": "^2.0.4",
		"read-pkg-up": "^7.0.0",
		"rimraf": "^3.0.0",
		"which": "^2.0.2",
		"yargs-parser": "^16.1.0"
		{{#ifCond typeSystem '===' 'flow'}}
		,"flow-bin": "^0.112.0"
		{{/ifCond}}
		{{#ifCond documentation '===' 'docz'}}
		,"docz": "^2.1.1"
			{{#ifCond typeSystem '===' 'flow'}}
			,"@babel/preset-flow": "^7.7.4"
			{{/ifCond}}
			{{#ifCond style '===' 'less'}}
			,"gatsby-plugin-less": "^3.0.15"
			{{/ifCond}}
			{{#ifCond style '===' 'scss'}}
			,"gatsby-plugin-sass": "2.1.17"
			{{/ifCond}}
		{{/ifCond}}
		{{#ifCond style '===' 'emotion'}}
		,"eslint-plugin-emotion": "^10.0.14",
		"babel-plugin-emotion": "^10.0.23",
		"@emotion/core": "^10.0.22"
		{{/ifCond}}
		{{#ifCond style '===' 'styled-component'}}
		,"styled-components": "^4.4.1",
		"babel-plugin-styled-components": "^1.10.6"
			{{#ifCond typeSystem '===' 'typescript'}}
			,"@types/styled-components": "^4.4.0"
			{{/ifCond}}
		{{/ifCond}}
		{{#ifCond documentation '===' 'storybook'}}
			,"@storybook/addon-actions": "^5.2.6",
			"@storybook/addon-docs": "^5.2.6",
			"@storybook/addon-links": "^5.2.6",
			"@storybook/addons": "^5.2.6",
			"babel-loader": "^8.0.6",
			"@storybook/react": "^5.2.6"
			{{#ifCond typeSystem '===' 'typescript'}}
			,"awesome-typescript-loader": "^5.2.1",
			"react-docgen-typescript-loader": "^3.6.0"
			{{/ifCond}}
			{{#if externalCSS}}
			,"css-loader": "^3.2.1",
			"style-loader": "^1.0.1"
			{{/if}}
			{{#ifCond style '===' 'scss'}}
			,"sass-loader": "^8.0.0"
			{{/ifCond}}
			{{#ifCond style '===' 'less'}}
			,"less-loader": "^5.0.0"
			{{/ifCond}}
		{{/ifCond}}
		{{#ifCond documentation '===' 'react-styleguidist'}}
			,"react-styleguidist": "^10.2.1",
			"webpack-blocks": "^2.0.1"
			{{#ifCond typeSystem '===' 'typescript'}}
			,"@webpack-blocks/typescript": "^2.0.1",
			"react-docgen-typescript": "^1.16.1"
			{{/ifCond}}
			{{#ifCond style '===' 'less'}}
			,"less-loader": "^5.0.0"
			,"webpack-blocks-less": "^0.2.0"
			{{/ifCond}}
			{{#ifCond style '===' 'scss'}}
			,"sass-loader": "^8.0.0"
			{{/ifCond}}
		{{/ifCond}}
		{{#ifCond typeSystem '===' 'typescript'}}
			,"@babel/preset-typescript": "^7.7.4",
			"@babel/types": "^7.7.4",
			"@types/jest": "^24.0.23",
			"@types/node": "^12.12.12",
			"@types/react": "^16.9.13",
			"@types/react-dom": "^16.9.4",
			"@types/react-test-renderer": "^16.9.1",
			"@typescript-eslint/eslint-plugin": "^2.8.0",
			"@typescript-eslint/parser": "^2.8.0",
			"ts-jest": "^24.2.0",
			"typescript": "^3.7.2"
		{{/ifCond}}
		{{#ifCond typeSystem '===' 'flow'}}
		,"@babel/preset-flow": "^7.7.4",
		"flow-typed": "^2.6.2"
		{{/ifCond}}
		{{#if externalCSS}}
		,"autoprefixer": "^9.7.3",
		"babel-plugin-transform-require-ignore": "^0.1.1",
		"postcss-preset-env": "^6.7.0",
		"rollup-plugin-postcss": "^2.0.3"
		{{/if}}
		{{#ifCond style '===' 'scss'}}
		,"node-sass": "^4.13.0"
		{{/ifCond}}
		{{#ifCond style '===' 'less'}}
		,"less": "^3.10.3"
		{{/ifCond}}
		{{#ifCond documentation '!==' 'none'}}
		,"gh-pages": "^2.1.1"
		{{/ifCond}}
		
		
	},
	"keywords": [
		"react",
		"{{name}}",
		"library"
	]
}
